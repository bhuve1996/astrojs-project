{
  "name": "windscribe-astro-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css,scss,md,astro}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css,scss,md,astro}\"",
    "format:css": "node scripts/format-css.js",
    "fix:css": "node scripts/fix-css-syntax.js",
    "comment:css": "node scripts/add-css-comments.js",
    "clean:css": "node scripts/clean-and-comment-css.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "astro": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.29.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "prettier-plugin-astro": "^0.12.0"
  }
}
